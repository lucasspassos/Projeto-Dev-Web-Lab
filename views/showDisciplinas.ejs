<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My CRUD in NodeJS</title>
</head>
<body>

<h2>Disciplinas<h2>
    <table border="1">
        <thead>
            <tr>
                <td>Disciplina nome</td>
                <td>Disciplina sobrenome</td>
            </tr>
        </thead>
        <tbody>
        <% data.forEach(function(disciplina) { %>
            <tr>
                <td><%= disciplina.nome %></td>
                <td><%= disciplina.sobrenome %></td>
                <td><a href="/edit/<%= disciplina._id %>">Editar</a> - <a href="/delete/<%= disciplina._id %>">Deletar</a></td>
            </tr>
            <% }) %>
        </tbody>
        </table>
        <button><a href="/">Novo</a></button>
</body>
</html>