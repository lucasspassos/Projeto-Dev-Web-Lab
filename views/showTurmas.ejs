<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My CRUD in NodeJS</title>
</head>
<body>

<h2>Turmas<h2>
    <table border="1">
        <thead>
            <tr>
                <td>Turma nome</td>
                <td>Turma sobrenome</td>
            </tr>
        </thead>
        <tbody>
        <% data.forEach(function(turma) { %>
            <tr>
                <td><%= turma.nome %></td>
                <td><%= turma.sobrenome %></td>
                <td><a href="/edit/<%= turma._id %>">Editar</a> - <a href="/delete/<%= turma._id %>">Deletar</a></td>
            </tr>
            <% }) %>
        </tbody>
        </table>
        <button><a href="/">Novo</a></button>
</body>
</html>